<tr data-sha="<%= c.sha ?? '-'%>" data-duration="<%= c.duration ?? '0'%>" data-date="<%= c.date ?? '0'%>">
  <td><%= new Date(c.date).toLocaleString('fr-CH') %></td>
  <td><%= c.name %></td>
  <td style="white-space: pre-wrap"><%= c.description || '' %></td>
  <td class="muted"><%= Math.floor((c.duration||0)/60) %>h <%= (c.duration||0)%60 %>m</td>
  <td class="muted"><span class="badge"><%= c.status || '' %></span></td>
  <td class="muted"><%= c.author %></td>
  <% if (c.sha) { %>
  <td style="border: none" class="muted" style="font-family: monospace">
    <a href="<%= c.url %>" target="_blank" rel="noopener noreferrer" style="text-decoration: none"> â¤´ </a>
  </td>
  <% } %>
</tr>
